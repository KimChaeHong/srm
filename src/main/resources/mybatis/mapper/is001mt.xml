<mapper namespace="com.birdie.srm.dao.IS001MTDao">
	<select id="selectInstAll" resultType="IS001MT">
		select INST_ID, INST_NM
		from TB_IS_001MT
		where INST_ID != 'OTI3' and DELT_YN = 'N'
	</select>

</mapper>